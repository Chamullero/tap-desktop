<page:StandardPage x:Class="TheAirlineV2.GraphicsModel.PageModel.PageGameModel.PageFrontMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:page="clr-namespace:TheAirlineV2.GraphicsModel.PageModel.GeneralModel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="PageFrontMenu">
    <Page.Resources>
        <Style x:Key="MenuEntryStyle" TargetType="Run">
            <Setter Property="FontSize" Value="24"></Setter>
            <Setter Property="FontWeight" Value="Bold"></Setter>
            <Setter Property="TextDecorations" Value="Underline"></Setter>
            <Setter Property="Foreground" Value="{DynamicResource HeaderBackgroundBrush}"></Setter>
            <EventSetter Event="MouseEnter" Handler="run_MouseEnter"></EventSetter>
            <EventSetter Event="MouseLeave" Handler="run_MouseLeave"></EventSetter>

        </Style>
    </Page.Resources>
        <Grid>
        
    </Grid>
</page:StandardPage>
