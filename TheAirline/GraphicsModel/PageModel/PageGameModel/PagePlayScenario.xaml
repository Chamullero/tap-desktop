<page:StandardPage x:Class="TheAirline.GraphicsModel.PageModel.PageGameModel.PagePlayScenario"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:page="clr-namespace:TheAirline.GraphicsModel.PageModel.GeneralModel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="PagePlayScenario">
    <Page.Resources>
        <DataTemplate x:Key="ScenarioItem">
            <TextBlock Width="100" VerticalAlignment="Bottom">
                    <Hyperlink Tag="{Binding}" Click="lnkScenario_Click">
                        <Run Text="{Binding Name}"></Run>
                    </Hyperlink>
            </TextBlock>
        </DataTemplate>
    </Page.Resources>
        <Grid>
        
    </Grid>
 </page:StandardPage>